<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="../../css/crypto.css">
</head>
<body><div class="row"><div class="col-6">
   <h1>Best Buys Right Now</h1>
    <table class="table table2">
        <thead>
          <tr>
            <th scope="col">Symbol</th>
            <th scope="col">Price</th>
            <th scope="col">24h High</th>
            <th scope="col">24h Low</th>
            <th scope="col">% To High</th>

          </tr>
        </thead>
        <tbody>
            <% for (let i =0; i < final.length; i++){var current = parseFloat(final[i].current);var high = parseFloat(final[i].high);var low = parseFloat(final[i].low);%>
          <% if (parseFloat((high-current)*(100/current)).toFixed(2) >35){  %> 
                <tr>
                    <td><%= final[i].id %></td>
                    <td><%= current %></td>
                    <td class="green"><%= high %></td>
                    <td class="red"><%= low %></td>
                    <td><%= parseFloat((high-current)*(100/current)).toFixed(2) %> %</td>
          </tr>
          <%}}%>
        </tbody>
      </table></div>
      <div class="col-6">
      <h1>Best Boys Right Now</h1>
    <table class="table table2">
        <thead>
          <tr>
            <th scope="col">Symbol</th>
            <th scope="col">Price</th>
            <th scope="col">24h High</th>
            <th scope="col">24h Low</th>
            <th scope="col">% To High</th>

          </tr>
        </thead>
        <tbody>
            <% for (let i =0; i < bigboy.length; i++){var currento = parseFloat(bigboy[i].current);var higho = parseFloat(bigboy[i].high);var lowo = parseFloat(bigboy[i].low);%>
              <% if (parseFloat((higho-currento)*(100/currento)).toFixed(2) >8) {  %> 
                <tr>
                    <td><%= bigboy[i].id %></td>
                    <td><%= currento %></td>
                    <td class="green"><%= higho %></td>
                    <td class="red"><%= lowo %></td>
                    <td><%= parseFloat((higho-currento)*(100/currento)).toFixed(2) %> %</td>
          </tr>
          <%}}%>
        </tbody>
      </table></div>
    </div>
    <h1 id="full">All Big Boys</h1>
    <table class="table table1">
        <thead>
          <tr>
            <th scope="col">Symbol</th>
            <th scope="col">Price</th>
            <th scope="col">24h High</th>
            <th scope="col">24h Low</th>
            <th scope="col">% To High</th>
            <th scope="col">24h-48h High/Low</th>
            <th scope="col">48h-72h High/Low</th>
            <th scope="col">72h-96h High/Low</th>

          </tr>
        </thead>
        <tbody>
            <% for (let i =0; i < bigboy.length; i++){var currento = parseFloat(bigboy[i].current);var higho = parseFloat(bigboy[i].high);var lowo = parseFloat(bigboy[i].low);%>
  
                <tr>
                    <td><%= bigboy[i].id %></td>
                    <td><%= currento %></td>
                    <td class="green"><%= higho %></td>
                    <td class="red"><%= lowo %></td>
                    <td><%= parseFloat((higho-currento)*(100/currento)).toFixed(2) %> %</td>
                    <td><%= parseFloat(100*(bigboy[i].high1-bigboy[i].low1)/bigboy[i].low1).toFixed(2) %> %</td>
                    
                    <td><%= parseFloat(100*(bigboy[i].high2-bigboy[i].low2)/bigboy[i].low2).toFixed(2)%> %</td>
                    
                    <td><%= parseFloat(100*(bigboy[i].high3-bigboy[i].low3)/bigboy[i].low3).toFixed(2) %> %</td>
                    
          </tr>
          <%}%>
        </tbody>
      </table>
    </div>
      <h1 id="full">Full List</h1>
      <table class="table table1">
          <thead>
            <tr>
              <th scope="col">Symbol</th>
              <th scope="col">Price</th>
              <th scope="col">24h High</th>
              <th scope="col">24h Low</th>
              <th scope="col">% To High</th>
              <th scope="col">24h-48h High/Low</th>
              <th scope="col">48h-72h High/Low</th>
              <th scope="col">72h-96h High/Low</th>
  
            </tr>
          </thead>
          <tbody>
              <% for (let i =0; i < final.length; i++){var current = parseFloat(final[i].current);var high = parseFloat(final[i].high);var low = parseFloat(final[i].low);%>
    
                  <tr>
                      <td><%= final[i].id %></td>
                      <td><%= current %></td>
                      <td class="green"><%= high %></td>
                      <td class="red"><%= low %></td>
                      <td><%= parseFloat((high-current)*(100/current)).toFixed(2) %> %</td>
                      <td><%= parseFloat(100*(final[i].high1-final[i].low1)/final[i].low1).toFixed(2) %> %</td>
                      
                      <td><%= parseFloat(100*(final[i].high2-final[i].low2)/final[i].low2).toFixed(2)%> %</td>
                      
                      <td><%= parseFloat(100*(final[i].high3-final[i].low3)/final[i].low3).toFixed(2) %> %</td>
                      
            </tr>
            <%}%>
          </tbody>
        </table>



<script src="https://code.jquery.com/jquery-3.5.1.min.js"   integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="   crossorigin="anonymous"></script>
